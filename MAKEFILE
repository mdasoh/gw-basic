	GIOSCN.OBJ GIOTBL.OBJ GWDATA.OBJ GWEVAL.OBJ GWINIT.OBJ GWLIST.OBJ \
	GWMAIN.OBJ GWRAM.OBJ GWSTS.OBJ IBMRES.OBJ ITSA86.OBJ KANJ86.OBJ \
	MACLNG.OBJ MATH.OBJ MATH.OBJ NEXT86.OBJ SCNDRV.OBJ \
	SCNEDT.OBJ OEM.OBJ
	link @gwbasic.lnk

